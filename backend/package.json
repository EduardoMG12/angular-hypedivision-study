		"reflect-metadata": "^0.2.2",
		"rxjs": "^7.8.1",
		"typeorm": "^0.3.20"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@eslint/eslintrc": "^3.2.0",
		"@eslint/js": "^9.18.0",
		"@nestjs/cli": "^11.0.0",
		"@nestjs/schematics": "^11.0.0",
		"@nestjs/testing": "^11.0.1",
		"@swc/cli": "^0.6.0",
		"@swc/core": "^1.10.7",
		"@types/express": "^5.0.0",
		"@types/jest": "^29.5.14",
		"@types/node": "^22.10.7",
		"@types/supertest": "^6.0.2",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
